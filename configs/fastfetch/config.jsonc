{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
     "source": "$(find ~/.config/fastfetch/pngs/ -name \"*.png\" -o -name \"*.jpeg\" -o -name \"*.jpg\" | shuf -n 1)",
	    "height": 11,
        "padding": {
            "top": 1,
            "right": 4
        }
    },
    "display": {
        "separator": " "
    },
    "modules": [
	"break",
                 {
            "type": "title",
            "keyWidth": 10,
			"format": "      {6}{7}{8}"
        },
        {
            "type": "custom",
            "format": "─────────────  ──────────────"
        },
        {
            "type": "os",
            "key": "󰣇 ",
            "keyColor": "blue",  // = color3
        },
        {
            "type": "kernel",
            "key": " ",
            "keyColor": "#50FA7B",
        },
        {
            "type": "packages",
            "format": "{} (pacman)",
            "key": " ",
            "keyColor": "#00FFFF",
        },
        {
            "type": "shell",
            "key": " ",
            "keyColor": "blue",
        },
        {
            "type": "terminal",
            "key": " ",
            "keyColor": "#50FA7B",
        },
        {
            "type": "wm",
            "key": " ",
            "keyColor": "#00FFFF",
        },
        {
            "type": "uptime",
            "key": " ",
            "keyColor": "blue",
        },
                {
            "type": "custom",
            "format": "─────────────  ──────────────"
        },
         {
            "type": "custom",
            "format": "  \u001b[31m  \u001b[32m  \u001b[33m  \u001b[34m  \u001b[35m  \u001b[36m  \u001b[37m  \u001b[90m "
        },
    ]
}
